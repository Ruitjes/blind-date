{
  "Routes": [
    {
      "DownstreamPathTemplate": "/{everything}",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "localhost",
          "Port": 7001
        }
      ],
      "UpstreamPathTemplate": "/question-service/{everything}",
      "SwaggerKey": "question"
    },
    {
      "DownstreamPathTemplate": "/{everything}",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "localhost",
          "Port": 7002
        }
      ],
      "UpstreamPathTemplate": "/feed-service/{everything}",
      "SwaggerKey": "feed"
    }
  ],
  "SwaggerEndPoints": [
    {
      "Key": "question",
      "Config": [
        {
          "Name": "Question Service API",
          "Version": "v1",
          "Url": "http://localhost:7001/swagger/v1/swagger.json"
        }
      ]
    },
    {
      "Key": "feed",
      "Config": [
        {
          "Name": "Feed Service API",
          "Version": "v1",
          "Url": "http://localhost:7002/swagger/v1/swagger.json"
        }
      ]
    }
  ],
  "GlobalConfiguration": {
    "BaseUrl": "http://localhost:7000"
  }
}
